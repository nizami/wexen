{
  "arrowParens": "always",
  "bracketSpacing": false,
  "endOfLine": "lf",
  "printWidth": 110,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "overrides": [
    {
      "files": ["**/.barrelize"],
      "options": {
        "parser": "json"
      }
    }
  ]
}
